<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape
            android:shape="oval">
            <solid
                android:color="@color/white"/>
            <size
                android:height="3dp"
                android:width="3dp"/>
            <stroke
                android:color="#4CA1E1"
                android:width=".8dp"/>
        </shape>
    </item>

    <item
        android:drawable="@drawable/baseline_bedtime_24"
        android:top="3dp"
        android:bottom="3dp"
        android:right="3dp"
        android:left="3dp" />

</layer-list>